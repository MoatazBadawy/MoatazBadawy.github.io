    "kanzHasanat": [
        {
            "hadith": "قال النبي صلى الله عليه وسلم: «مَنْ صَلَّى عَلَيَّ صَلَاةً وَاحِدَةً صَلَّى اللَّهُ عَلَيْهِ عَشْرَ صَلَوَاتٍ، وَحُطَّتْ عَنْهُ عَشْرُ خَطِيئَاتٍ، وَرُفِعَتْ لَهُ عَشْرُ دَرَجَاتٍ»",
            "authorName": "رواه النسائي"
        }
    ],
    "counter": [
        {
            "text": "اللهم صلِّ وسلم\n وبارك على نبينا محمد"
        }
    ],
    "tahzeebMuslim": [
        {
            "hadith": "عن عبد الله بن عمر -رضي الله عنه- أنه قال: (لَمْ يَكُنْ رَسولُ اللَّهِ -صَلَّى اللهُ عليه وسلَّمَ- فَاحِشًا ولَا مُتَفَحِّشًا، وإنَّه كانَ يقولُ: إنَّ خِيَارَكُمْ أحَاسِنُكُمْ أخْلَاقًا)",
            "authorName": "رواه مسلم"
        }
    ],
